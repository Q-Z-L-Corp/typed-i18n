{
	"name": "@qzlcorp/typed-i18n-react",
	"version": "1.0.0",
	"description": "React bindings for @qzlcorp/typed-i18n",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.tsx --format cjs,esm --dts --external react --external react-dom",
		"test": "vitest run",
		"test:watch": "vitest",
		"clean": "rm -rf dist"
	},
	"keywords": [
		"i18n",
		"react",
		"internationalization",
		"typescript",
		"hooks"
	],
	"author": "Errong Leng <errong.leng@gmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"@qzlcorp/typed-i18n": "^1.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@qzlcorp/typed-i18n": "workspace:*",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^14.3.1",
		"@types/react": "^18.2.0",
		"@vitejs/plugin-react": "^5.1.1",
		"happy-dom": "^20.0.11",
		"jsdom": "^27.2.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tsup": "^8.5.1",
		"typescript": "^5.0.0",
		"vitest": "^4.0.14"
	}
}
