{
	"name": "@qzlcorp/typed-i18n",
	"version": "1.0.3",
	"description": "Type-safe i18n library with zero dependencies",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"test": "vitest run",
		"test:watch": "vitest",
		"clean": "rm -rf dist"
	},
	"keywords": [
		"i18n",
		"internationalization",
		"typescript",
		"type-safe",
		"translations"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Q-Z-L-Corp/typed-i18n.git"
	},
	"homepage": "https://github.com/Q-Z-L-Corp/typed-i18n#readme",
	"bugs": {
		"url": "https://github.com/Q-Z-L-Corp/typed-i18n/issues"
	},
	"author": "Q.Z.L Corp.",
	"contributors": [
		"Errong Leng <errong.leng@gmail.com>"
	],
	"license": "MIT",
	"devDependencies": {
		"tsup": "^8.5.1",
		"typescript": "^5.0.0",
		"vitest": "^4.0.14"
	}
}
